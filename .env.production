NODE_ENV=production
NEXTAUTH_URL=https://www.fixral.com
# WARNING: Do NOT commit real secrets. Set these via hosting provider env vars.
NEXTAUTH_SECRET=__SET_IN_ENV_PROVIDER__
MONGODB_URI=__SET_IN_ENV_PROVIDER__

# Security Settings
ADMIN_DEFAULT_PASSWORD=__SET_IN_ENV_PROVIDER__
SECURITY_HEADERS_ENABLED=true
CSRF_PROTECTION_ENABLED=true

# Cloudinary
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

APP_NAME=Personal Blog
APP_URL=https://www.fixral.com
RATE_LIMIT_MAX=50
RATE_LIMIT_WINDOW=900000

# Production Settings
BYPASS_RATE_LIMIT=false
NEXT_PUBLIC_SHOW_SKELETON=false

# Vercel Settings
VERCEL_LIVE_FEEDBACK=false
VERCEL=1
SKIP_ENV_VALIDATION=true
NEXT_TELEMETRY_DISABLED=1