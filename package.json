{
  "name": "personal-blog",
  "version": "2.4.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next && rimraf out",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:config": "node scripts/test-config.js",
    "security:check": "npm audit",
    "security:test": "npm audit --audit-level=moderate",
    "perf:check": "next build --profile",
    "perf:analyze": "ANALYZE=true next build",
    "build:analyze": "ANALYZE=true next build",
    "vercel-build": "next build",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "sync-videos": "node scripts/sync-youtube-videos.js",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "monitoring:test": "curl -f http://localhost:3000/api/monitoring/performance || exit 1",
    "monitoring:dashboard": "open http://localhost:3000/admin/monitoring"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.6",
    "framer-motion": "^10.16.4",
    "jsdom": "^23.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.1.0",
    "mongoose": "^7.5.2",
    "next": "14.2.30",
    "next-auth": "^4.23.1",
    "nodemailer": "^6.9.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sanitize-html": "^2.11.0",
    "sharp": "^0.32.6",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.0.0",
    "@types/bcryptjs": "^2.4.4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "20.6.2",
    "@types/node-fetch": "^2.6.13",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/sanitize-html": "^2.9.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "autoprefixer": "^10.4.15",
    "dotenv": "^16.3.1",
    "eslint": "8.49.0",
    "eslint-config-next": "14.2.30",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.0",
    "typescript": "5.6.3"
  }
